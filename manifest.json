{
  "name": "UofT Timetable Improver",
  "version": "0.3.0",
  "description": "Improves the UTSG timetable webpages.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://www.artsandscience.utoronto.ca/ofr/timetable/winter/*"],
      "css": ["css/coursepage.css"],
      "js": ["js/jquery-2.1.1.min.js", "js/contentscript.js"]
    }
  ],
  "icons": { "16": "img/icon16.png", "48": "img/icon48.png", "128": "img/icon128.png" }
}
